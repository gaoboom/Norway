@model Norway.Models.FeedBack
@{
    ViewBag.Title = "FeedBack";
}

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h2><span class="label label-primary" style="display:block;">意见及建议</span></h2>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            @using (Html.BeginForm())
            {
                <p>Name:@Html.TextBoxFor(x=>x.Name)</p>
                <p>Title:@Html.TextBoxFor(x=>x.Title)</p>
                <p>Content:@Html.TextAreaFor(x=>x.Content)</p>
                <p>Contact:@Html.TextBoxFor(x=>x.Contact)</p>
                <button type="submit" class="btn btn-default">提交</button>
            }
        </div>
    </div>
</div>

